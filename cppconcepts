#include<bits/stdc++.h>
using namespace std;
int main()
{
    vector<int>v;
    cout<<v.size()<<endl;
    // for(int i=0 ;i<5;i++)
    // {
    //     v.push_back(i);
    //     cout<<v[i];
    // }
    v.push_back(1);
    cout<<v.size()<<endl;
    cout<<v.capacity()<<endl;
    v.push_back(2);
    cout<<v.size()<<endl;
    cout<<v.capacity()<<endl;
    v.push_back(3);
    cout<<v.size()<<endl;
    cout<<v.capacity()<<endl;
    v.push_back(14);
    cout<<v.size()<<endl;
    cout<<v.capacity()<<endl;
    v.push_back(5);
    cout<<v.size()<<endl;
    cout<<v.capacity()<<endl;
    v.push_back(3);
    cout<<v.size()<<endl;
    cout<<v.capacity()<<endl;
    v.push_back(3);
    cout<<v.size()<<endl;
    cout<<v.capacity()<<endl;
    v.push_back(3);
    cout<<v.size()<<endl;
    cout<<v.capacity()<<endl;
    v.push_back(3);
    cout<<v.size()<<endl;
    cout<<v.capacity()<<endl;
    return 0;
}


